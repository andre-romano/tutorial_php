{
    "cSpell.enabled": true,
    "cSpell.language": "en,pt_BR",
    "cSpell.ignoreWords": [],
    "cSpell.words": [
        "Intelephense",
        "printscreen",
        "IFBA",
        "ifba"
    ],
    "editor.wordWrap": "on",
    "editor.wordWrapColumn": 80,
    "search.exclude": {
        "**/node_modules": true,
        "**/bower_components": true,
        "**/*.code-search": true,
        "**/.phpunit.cache": true,
        "**/.phpunit.result.cache": true,
        "**/vendor": true,
    }
}